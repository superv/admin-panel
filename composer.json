{
    "name": "superv/admin-panel",
    "type": "superv-panel",
    "autoload": {
        "psr-4": {
            "SuperV\\Panels\\Admin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "require": {
        "superv/platform": "^0.25"
    },
    "minimum-stability": "dev",
    "config": {
        "allow-plugins": {
            "kylekatarnls/update-helper": true,
            "wikimedia/composer-merge-plugin": true,
            "superv/composer-plugin": true
        }
    }
}
